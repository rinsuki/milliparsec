{
  "name": "milliparsec",
  "version": "2.0.5",
  "description": "tiniest body parser in the universe",
  "repository": "https://github.com/talentlessguy/parsec.git",
  "author": "talentlessguy <pilll.PL22@gmail.com>",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "type": "module",
  "keywords": [
    "body-parser",
    "express",
    "http",
    "koa",
    "body-parsing"
  ],
  "engines": {
    "node": ">=12.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./koa": {
      "import": "./dist/koa.js",
      "require": "./dist/koa.cjs"
    },
    "./": "./"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@tinyhttp/app": "^1.3.1",
    "@types/koa": "^2.13.1",
    "@types/node": "^15.0.1",
    "c8": "^7.7.2",
    "express": "^4.17.1",
    "koa": "^2.13.1",
    "rollup": "^2.46.0",
    "supertest-fetch": "^1.4.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "uvu": "^0.5.1"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "uvu -r ts-node/register tests",
    "test:coverage": "c8 --include=src pnpm test",
    "test:report": "c8 report --reporter=text-lcov > coverage.lcov",
    "build": "rollup -c",
    "prepare": "pnpm build && pnpm test"
  },
  "optionalDependencies": {
    "koa": "^2.13.1"
  }
}
